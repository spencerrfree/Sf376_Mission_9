@model BooksViewModel
<h1>The Book Store</h1>



@foreach (Book b in Model.Books)
{
<div class="card card-outline-primary m-1 p-1">
    <div class="bg-faded p-1">
        <h4>
            @b.Title
            <span class="badge bg-primary" style="float:right">
                <small>$@b.Price</small>
            </span>
        </h4>
        <div class="card-text p-1">
            <ul>
                <li>Author: @b.Author</li>
                <li>Publisher: @b.Publisher</li>
                <li>IBSN: @b.Isbn</li>
                <li>Classification/Category: @b.Classification</li>
                <li>Number of Pages: @b.PageCount</li>
            </ul>
        </div>
    </div>
</div>

}

<div page-blah="@Model.PageInfo" page-action="Index" page-classes-enabled="true"page-class="btn" page-class-normal="btn-outline-dark"
     page-class-selected="btn-primary" class="btn-group pull-right m-1">
</div>